@model Corum.Models.ViewModels.Admin.FAQAnswersViewModel

<div class="form-group">
    @Html.LabelFor(model => model.Question, htmlAttributes: new { @class = "control-label col-md-1" })
    <div class="col-md-10">

        @Html.TextAreaFor(model => model.Question,
                                          new { @class = "form-control input col-md-10" })
        @Html.ValidationMessageFor(model => model.Question, "", new { @class = "text-danger" })

    </div>
</div>


<div class="form-group">
    @Html.LabelFor(model => model.Answer, htmlAttributes: new { @class = "control-label col-md-1" })
    <div class="col-md-10">
        @Html.TextAreaFor(model => model.Answer, new { @class = "form-control input htmlEditor" })

        @Html.ValidationMessageFor(model => model.Answer, "", new { @class = "text-danger" })
    </div>
</div>
