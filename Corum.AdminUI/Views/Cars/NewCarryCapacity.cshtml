@model Corum.Models.ViewModels.Cars.CarryCapacitiesViewModel
@{
    ViewBag.Title = "NewCarryCapacity";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@using (Html.BeginForm("NewCarryCapacity", "Cars", FormMethod.Post))
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <h4>Добавление нового типа грузоподъемностей</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        @Html.HiddenFor(model => model.Id, new { htmlAttributes = new { @class = "form-control" } })

            @Html.Partial("_CarryCapacityDetails")
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Создать" class="btn btn-default" />
                </div>
            </div>
    </div>
}







