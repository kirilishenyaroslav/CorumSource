@model Corum.Models.ViewModels.Cars.RouteIntervalTypesViewModel

<div class="form-group" style="margin-top: 20px;">
    @Html.LabelFor(model => model.IntervalTypeName, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(model => model.IntervalTypeName, new { htmlAttributes = new { @class = "form-control", autocomplete = "off" } })
        @Html.ValidationMessageFor(model => model.IntervalTypeName, "", new { @class = "text-danger" })
    </div>
</div>


<div class="form-group">
    @Html.LabelFor(model => model.MaxInterval, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(model => model.MaxInterval, new { htmlAttributes = new { @class = "form-control", autocomplete = "off" } })
        @Html.ValidationMessageFor(model => model.MaxInterval, "", new { @class = "text-danger" })
    </div>
</div>

