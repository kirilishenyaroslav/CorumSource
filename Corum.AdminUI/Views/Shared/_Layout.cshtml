<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" href="~/Content/img/logo_icon_favicon.png" />
    <title>Corum source</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/ui_css")
    @Styles.Render("~/Content/JQWidgetsCss")
    <link rel="stylesheet" href="~/Content/themes/base/jquery-ui.css">
    <link rel="stylesheet" href="/resources/demos/style.css">
    @Scripts.Render("~/scripts/jquery-1.12.4.js")
    @Scripts.Render("~/scripts/jquery-ui-1.12.0.js")


    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/grathVis")

    @RenderSection("head_scripts", required: false)

    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,300italic,300,400italic,600italic,600,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
    <meta name="description" content="Corum" />
</head>
<body>
    @Html.Hidden("MasterId")

    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="fluid-container">
            <div class="row" style="background: none 0% 0% repeat scroll rgb(239, 125, 26);">
                <div class="col-lg-12">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="/">
                            <div class="logo-icon pull-left"></div>
                        </a>
                    </div>
                    <div class="navbar-collapse collapse">
                            @{ Html.RenderAction("MainMenu", "Access"); }    
                            @Html.Partial("_Toastr")
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="container-fluid">
        @RenderBody()
    </div>

    <div id="delete-dialog" style="display:none;" class="panel modal_dialog">
        <span id="confirmation_message"></span>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryUI")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/jquery_ajax")
    @Scripts.Render("~/bundles/blockUI")
    @Scripts.Render("~/bundles/datetime")
    @Scripts.Render("~/bundles/select2")
    @Scripts.Render("~/bundles/MvcGrid")
    @Scripts.Render("~/bundles/toastr")
    @Scripts.Render("~/bundles/corum")
    @Scripts.Render("~/bundles/jqwidgets")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/tinymce")

    @RenderSection("scripts", required: false)
</body>
</html>


