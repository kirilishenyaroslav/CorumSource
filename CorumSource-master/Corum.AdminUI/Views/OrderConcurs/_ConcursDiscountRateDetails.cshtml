@model Corum.Models.ViewModels.OrderConcurs.ConcursDiscountRateModel
@{
    ViewBag.Title = "_ConcursDiscountRateDetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="form-group" style="margin-top: 20px;">
    @Html.LabelFor(model => model.DiscountRate, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-4" style="padding-left: 0px;">
        @Html.EditorFor(model => model.DiscountRate, new {htmlAttributes = new {@class = "form-control", autocomplete = "off"}})
        @Html.ValidationMessageFor(model => model.DiscountRate, "", new {@class = "text-danger"})
    </div>
    @Html.HiddenFor(model => model.DiscountRate)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.DateBeg, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="input-group">
        @Html.EditorFor(model => model.DateBeg, new { htmlAttributes = new { @class = "form-control datetimepicker client-info", @readonly = "true", autocomplete = "off", data_raw_element = "DateBegRaw" } })
        <span class="input-group-addon glyphicon glyphicon-calendar datetimepicker" data-raw-element="DateBegRaw" data-linked-input="DateBeg"></span>
        @Html.ValidationMessageFor(model => model.DateBeg, "", new { @class = "text-danger" })
    </div>
    @Html.HiddenFor(model => model.DateBeg)
    @Html.HiddenFor(model => model.DateBegRaw)
</div>
<div class="form-group">
    @Html.LabelFor(model => model.DateEnd, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="input-group">
        @Html.EditorFor(model => model.DateEnd, new {htmlAttributes = new {@class = "form-control datetimepicker client-info", @readonly = "true", autocomplete = "off", data_raw_element = "DateEndRaw"}})
        <span class="input-group-addon glyphicon glyphicon-calendar datetimepicker" data-raw-element="DateEndRaw" data-linked-input="DateEnd"></span>
        @Html.ValidationMessageFor(model => model.DateEnd, "", new {@class = "text-danger"})
    </div>
    @Html.HiddenFor(model => model.DateEnd)
    @Html.HiddenFor(model => model.DateEndRaw)
</div>

